<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="es.source.code.activity">

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="22" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.ecourse.activity.LoadActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ecourse.activity.CourseTable" />
        <activity android:name="com.ecourse.activity.Register" />
        <activity android:name="com.ecourse.activity.Login" />
        <activity android:name="com.ecourse.activity.NoteMainActivity" />
        <activity android:name="com.ecourse.activity.AddContent" />
        <activity android:name="com.ecourse.activity.SelectAct" />
        <activity android:name="com.ecourse.activity.About" />
        <activity android:name="com.ecourse.activity.Feedback" />
        <activity android:name="com.ecourse.activity.Info" />
    </application>

</manifest>